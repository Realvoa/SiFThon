        "        chars = [\"|\", \"/\", \"-\", \"\\\\\"]\n",
        "        start_time = time.time()\n",
        "        i = 0\n",
        "        \n",
        "        while time.time() - start_time < duration:\n",
        "            print(f\"\\r{chars[i % len(chars)]} {text}\", end=\"\", flush=True)\n",
        "            time.sleep(0.1)\n",
        "            i += 1\n",
        "        print(f\"\\r  {text} - مكتمل\")\n",
        "    \n",
