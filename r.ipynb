        "    \n",
        "    def run_script_advanced(self):\n",
        "        \"\"\"تشغيل السكربت بنظام متقدم\"\"\"\n",
        "        print(\"\\nبدء تشغيل SiFThon Pro...\")\n",
        "        print(\"=\" * 50)\n",
        "        \n",
        "        try:\n",
        "            # الانتقال إلى المجلد\n",
        "            os.chdir(self.base_dir)\n",
        "            \n",
        "            # تشغيل السكربت في عملية منفصلة\n",
        "            process = subprocess.Popen(\n",
        "                [sys.executable, '-m', 'SiFThon'],\n",
        "                stdout=subprocess.PIPE,\n",
        "                stderr=subprocess.STDOUT,\n",
        "                text=True,\n",
        "                bufsize=1,\n",
        "                universal_newlines=True\n",
        "            )\n",
        "            \n",
        "            # مراقبة المخرجات في الوقت الحقيقي\n",
        "            print(\"جاري مراقبة عملية التشغيل...\")\n",
        "            while True:\n",
        "                output = process.stdout.readline()\n",
        "                if output == '' and process.poll() is not None:\n",
        "                    break\n",
        "                if output:\n",
        "                    print(f\"  {output.strip()}\")\n",
        "            \n",
        "            # التحقق من حالة الخروج\n",
        "            return_code = process.poll()\n",
        "            if return_code == 0:\n",
        "                print(\"تم إنهاء السكربت بنجاح\")\n",
        "            else:\n",
        "                print(f\"انتهى السكربت برمز خروج: {return_code}\")\n",
