      "source": [
        "# @title تشغيل سورس سيفثون\n",
        "\n",
        "import os\n",
        "import requests\n",
        "import zipfile\n",
        "import subprocess\n",
        "\n",
        "print(\"=\" * 50)\n",
        "print(\"بدء تشغيل سورس سيفثون\")\n",
        "print(\"=\" * 50)\n",
        "\n",
        "try:\n",
        "    API_ID = api_id_input.value\n",
        "    API_HASH = api_hash_input.value\n",
        "    SESSION = session_code_input.value\n",
        "    BOT_TOKEN = bot_token_input.value\n",
        "except:\n",
        "    print(\"-\" * 40)\n",
        "    print(\"لم يتم تعيين المتغيرات\")\n",
        "    print(\"يرجى تعيين المتغيرات في القسم السابق\")\n",
        "    print(\"-\" * 40)\n",
        "    exit()\n",
        "\n",
        "print(\"جاري تحميل السورس...\")\n",
        "url = \"http://especi1.serv00.net/SiFThon.zip\"\n",
        "response = requests.get(url)\n",
        "with open(\"SiFThon.zip\", \"wb\") as f:\n",
        "    f.write(response.content)\n",
        "\n",
        "print(\"جاري فك الضغط...\")\n",
        "with zipfile.ZipFile(\"SiFThon.zip\", 'r') as zip_ref:\n",
        "    zip_ref.extractall(\".\")\n",
        "\n",
        "print(\"جاري التحقق من الملفات...\")\n",
        "!ls -la SiFThon/\n",
        "\n",
        "print(\"\\nجاري البحث عن ملفات البايثون...\")\n",
        "!find SiFThon -name \"*.py\" | head -10\n",
        "\n",
        "print(\"\\nجاري تثبيت المتطلبات...\")\n",
        "!pip install -q pyrogram telethon requests\n",
        "\n",
        "print(\"جاري إنشاء ملف الإعدادات...\")\n",
        "env_content = f\"\"\"API_ID={API_ID}\n",
        "API_HASH={API_HASH}\n",
        "SESSION={SESSION}\n",
        "BOT_TOKEN={BOT_TOKEN}\n",
        "\"\"\"\n",
        "\n",
        "# التأكد من وجود المجلد\n",
        "os.makedirs(\"SiFThon\", exist_ok=True)\n",
        "\n",
        "with open('SiFThon/.env', 'w') as file:\n",
        "    file.write(env_content)\n",
        "\n",
        "print(\"=\" * 50)\n",
        "print(\"بدء تشغيل السورس\")\n",
        "print(\"=\" * 50)\n",
        "\n",
        "# الانتقال إلى مجلد السورس\n",
        "os.chdir(\"SiFThon\")\n",
        "\n",
        "# محاولة تشغيل السورس بطرق مختلفة\n",
        "print(\\"جاري تشغيل السورس...\\")\n",
        "\n",
        "# الطريقة 1: البحث عن ملف التشغيل الرئيسي\n",
        "main_files = [\"main.py\", \"sython.py\", \"bot.py\", \"app.py\", \"run.py\", \"SiFThon.py\"]\n",
        "for file in main_files:\n",
        "    if os.path.exists(file):\n",
        "        print(f\"تم العثور على {file}، جاري التشغيل...\")\n",
        "        os.system(f\"python {file}\")\n",
        "        break\n",
        "else:\n",
        "    # الطريقة 2: تشغيل كوحدة\n",
        "    print(\"جاري التشغيل كوحدة...\")\n",
        "    try:\n",
        "        os.system(\"python -m sython\")\n",
        "    except:\n",
        "        # الطريقة 3: البحث عن أي ملف بايثون قابل للتشغيل\n",
        "        print(\"جاري البحث عن أي ملف تشغيل...\")\n",
        "        !find . -name \"*.py\" -exec grep -l \"if __name__\" {} \\; | head -5\n",
        "        \n",
        "        # محاولة تشغيل أول ملف بايثون\n",
        "        python_files = !find . -name \"*.py\" | head -1\n",
        "        if python_files:\n",
        "            print(f\"جاري تشغيل: {python_files[0]}\")\n",
        "            os.system(f\"python {python_files[0]}\")\n"
      ],
