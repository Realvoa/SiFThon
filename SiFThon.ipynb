    },
    {
      "cell_type": "code",
      "source": [
        "# @title تشغيل سورس سيفثون\n",
        "\n",
        "import os\n",
        "\n",
        "print(\"=\" * 50)\n",
        "print(\"بدء تشغيل سورس سيفثون\")\n",
        "print(\"=\" * 50)\n",
        "\n",
        "try:\n",
        "    API_ID = api_id_input.value if api_id_input.value else None\n",
        "    API_HASH = api_hash_input.value if api_hash_input.value else None\n",
        "    SESSION = session_code_input.value if session_code_input.value else None\n",
        "    BOT_TOKEN = bot_token_input.value if bot_token_input.value else None\n",
        "except NameError:\n",
        "    API_ID = None\n",
        "    API_HASH = None\n",
        "    SESSION = None\n",
        "    BOT_TOKEN = None\n",
        "\n",
        "try:\n",
        "    env_stats = [env_stats]\n",
        "except NameError:\n",
        "    env_stats = None\n",
        "\n",
        "def check_installation():\n",
        "    if not os.path.exists(\"SiFThon\"):\n",
        "        print(\"جاري تحميل ملفات السورس...\")\n",
        "        os.system(\"wget https://example.com/SiFThon.zip -O SiFThon.zip > /dev/null 2>&1\")\n",
        "        os.system(\"unzip -o SiFThon.zip -d SiFThon > /dev/null 2>&1\")\n",
        "        print(\"✓ تم تحميل ملفات السورس\")\n",
        "    \n",
        "    if not os.path.exists(\"lib/packages\"):\n",
        "        print(\"جاري تحميل المكتبات المطلوبة...\")\n",
        "        os.system(\"git clone -b lib https://github.com/MOA-YAD/0 lib > /dev/null 2>&1\")\n",
        "        print(\"✓ تم تحميل المكتبات\")\n",
        "    \n",
        "    if not os.path.exists(\"SiFThon/ytdl\"):\n",
        "        print(\"جاري إعداد البيئة...\")\n",
        "        os.system(\"sudo unzip lib/packages -d SiFThon > /dev/null 2>&1\")\n",
        "        os.system(\"cd SiFThon && sudo dpkg -i *.deb > /dev/null 2>&1 && sudo apt-get install -f > /dev/null 2>&1\")\n",
        "        print(\"✓ تم إعداد البيئة\")\n",
        "\n",
        "def contains_value(value, target):\n",
        "    if isinstance(target, list):\n",
        "        return any(contains_value(value, item) for item in target)\n",
        "    return target == value\n",
        "\n",
        "def setup_environment():\n",
        "    try:\n",
        "        if None in (SESSION, API_ID, API_HASH, BOT_TOKEN) and contains_value(0, env_stats):\n",
        "            print(\"-\" * 40)\n",
        "            print(\"لم يتم تعيين المتغيرات\")\n",
        "            print(\"يرجى تعيين المتغيرات في الأقسام السابقة\")\n",
        "            print(\"-\" * 40)\n",
        "            return False\n",
        "            \n",
        "        elif not None in (SESSION, API_ID, API_HASH, BOT_TOKEN) and contains_value(0, env_stats):\n",
        "            print(\"جاري إنشاء ملف المتغيرات...\")\n",
        "            env_content = f\"\"\"API_ID={API_ID}\n",
        "API_HASH={API_HASH}\n",
        "SESSION={SESSION}\n",
        "BOT_TOKEN={BOT_TOKEN}\n",
        "\"\"\"\n",
        "            with open('env', 'w') as file:\n",
        "                file.write(env_content)\n",
        "            print(\"✓ تم إنشاء ملف المتغيرات\")\n",
        "            \n",
        "    except (NameError, AttributeError):\n",
        "        pass\n",
        "    \n",
        "    if os.path.isfile(\"env\"):\n",
        "        print(\"جاري تعيين المتغيرات...\")\n",
        "        os.system(\"rm -rf ./SiFThon/.env && cp env ./SiFThon/.env\")\n",
        "        print(\"✓ تم تعيين المتغيرات\")\n",
        "        return True\n",
        "    else:\n",
        "        print(\"-\" * 40)\n",
        "        print(\"لم يتم العثور على ملف المتغيرات\")\n",
        "        print(\"-\" * 40)\n",
        "        return False\n",
        "\n",
        "def run_sifthon():\n",
        "    check_installation()\n",
        "    \n",
        "    print(\"-\" * 40)\n",
        "    print(\"التحضير النهائي للتشغيل...\")\n",
        "    print(\"-\" * 40)\n",
        "    \n",
        "    if setup_environment():\n",
        "        print(\"=\" * 50)\n",
        "        print(\"بدء تشغيل سورس سيفثون\")\n",
        "        print(\"=\" * 50)\n",
        "        \n",
        "        os.system(\"cd SiFThon && python -m SiFThon\")\n",
        "    else:\n",
        "        print(\"فشل في بدء التشغيل\")\n",
        "        print(\"يرجى التحقق من المتغيرات والمحاولة مرة أخرى\")\n",
        "\n",
        "run_sifthon()"
      ],
