        "            env_stats = 1\n",
        "        else:\n",
        "            print(\"-\" * 40)\n",
        "            print(\"خطأ: اسم الملف يجب أن يبدأ بـ 'env'\")\n",
        "            print(\"تم إلغاء عملية الرفع\")\n",
        "            os.remove(filename)\n",
        "\n",
        "except KeyboardInterrupt:\n",
        "    print(\"-\" * 40)\n",
        "    print(\"تم إلغاء عملية الرفع\")\n",
